{
  "name": "next-moment",
  "version": "1.0.1",
  "description": "",
  "main": "dist/NextMoment.js",
  "scripts": {
    "build:dist": "better-npm-run build:dist",
    "build:prod": "better-npm-run build:prod",
    "build": "shell-exec 'bnr build:dist' 'bnr build:prod'",
    "test": "better-npm-run test"
  },
  "betterScripts": {
    "build:dist": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha --reporter nyan --compilers js:babel-register test/**/*.test.js test/*.test.js",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "moment": "^2.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "better-npm-run": "0.0.9",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "shell-executor": "^0.3.2",
    "webpack": "^1.13.1"
  }
}
